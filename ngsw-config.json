{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/festival/index.html",
  "assetGroups": [
    {
      "name": "konfusius",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/public/manifest.webmanifest",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif|otf|ttf|woff|woff2|pdf)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api--requests",
      "urls": [
        "**/wc/v3/**",
        "**/wc/store/v1/**",
        "**/custom/v1/**",
        "**/wp/v2/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 50,
        "maxAge": "1h",
        "timeout": "10s"
      }
    }
  ]
}
